<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.life.mm.framework">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:supportsRtl="true">

        <activity android:name=".app.base.activity.MapActivity"
        android:screenOrientation="portrait"/>
        <!-- 登陆 -->
        <activity android:name=".app.login.LoginActivity"
            android:screenOrientation="portrait"/>
        <!-- 用户名与密码注册 -->
        <activity android:name=".app.register.RegisterActivity"
            android:screenOrientation="portrait"/>
        <!-- 手机号注册 -->
        <activity android:name=".app.register.PhoneRegisterActivity"
            android:screenOrientation="portrait"/>
        <!-- 邮箱注册 -->
        <activity android:name=".app.register.EmailRegisterActivity"
            android:screenOrientation="portrait"/>
        <!-- 密码设置 -->
        <activity android:name=".app.register.PasswordSetActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>
